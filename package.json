{
  "name": "Fluid-contact",
  "version": "1.0.0",
  "description": "An api for Fluid contact",
  "main": "app.js",
  "author": "Oluwatobi",
  "license": "ISC",
  "scripts": {
    "lint": "eslint src/ --fix",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register ./test/**/*.spec.js",
    "start": "NODE_ENV=development npm run clusters",
    "clusters": "babel-node cluster.js",
    "create-db": "sequelize db:create",
    "migrate": "sequelize db:migrate",
    "setup-db": "npm run create-db && npm run migrate",
    "create-migration": "sequelize migration:create --name ",
    "create-seed": "sequelize seed:generate --name ",
    "reset-db": "sequelize db:migrate:undo:all",
    "undo-migration": "sequelize db:migrate:undo",
    "seed-db": "sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git@https://github.com/PantherTec/product-api"
  },
  "dependencies": {
    "async": "^2.6.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.3",
    "express-validator": "^5.0.0",
    "helmet": "^3.13.0",
    "http-errors": "^1.7.1",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "node-fetch": "^1.7.3",
    "nodemailer": "^4.6.8",
    "nodemailer-express-handlebars": "^3.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.6.0",
    "pg-hstore": "^2.3.2",
    "regenerator-runtime": "^0.12.1",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^4.0.0",
    "sequelize-test-helpers": "^1.0.6",
    "sinon": "^7.1.1",
    "swagger-jsdoc": "^3.2.3",
    "swagger-model-validator": "^3.0.5",
    "swagger-ui-express": "^4.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": "10.0.0"
  }
}
